{
  "name": "loanpro",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:local": "next dev",
    "next:build": "next build",
    "start": "docker-compose up",
    "build": "docker-compose build",
    "clean": "npm run clean:images && npm run clean:volumes",
    "clean:images": "docker system prune -af",
    "clean:volumes": "docker volume prune -f",
    "seed": "docker exec -it app node ./prisma/seed",
    "migrate:save": "docker exec -it app prisma migrate save --experimental",
    "migrate:up": "docker exec -it app prisma migrate up --experimental",
    "migrate": "npm run migrate:save && npm run migrate:up"
  },
  "dependencies": {
    "@prisma/client": "^2.7.1",
    "decimal.js": "^10.2.0",
    "next": "9.5.3",
    "next-connect": "^0.9.0",
    "nookies": "^2.4.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.7.1"
  }
}
